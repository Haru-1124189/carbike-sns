{
  "name": "vehicle-catalog-collector",
  "version": "1.0.0",
  "description": "Wikidataから車種・バイク種データを収集してFirestoreに保存するツール",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "collect": "npm run build && node dist/collector/index.js",
    "upload": "npm run build && node dist/load/firestore.js",
    "all": "npm run collect && npm run upload"
  },
  "dependencies": {
    "firebase-admin": "^11.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "wikidata",
    "sparql",
    "firestore",
    "vehicles",
    "cars",
    "motorcycles"
  ],
  "author": "Your Name",
  "license": "MIT"
}
